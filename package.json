{
  "name": "goodpluckv2",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "wrangler pages dev ----compatibility-date 2023-11-14 --env development ./dist",
    "test:e2e": "astro build && pnpm exec playwright test --trace on",
    "astro": "astro",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,astro,html}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/cloudflare": "^7.6.4",
    "@astrojs/solid-js": "^3.0.2",
    "@astrojs/tailwind": "^5.0.2",
    "@motionone/solid": "^10.16.4",
    "@nanostores/solid": "^0.4.2",
    "astro": "^3.3.4",
    "nanostores": "^0.9.4",
    "solid-js": "^1.8.3",
    "stylelint": "^15.11.0",
    "stytch": "^9.0.6",
    "swell-js": "^3.24.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "typescript": "^5.2.2",
    "wrangler": "^3.18.0"
  }
}
